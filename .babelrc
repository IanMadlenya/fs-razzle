{
    "presets": [
        "razzle/babel"
    ],
    "plugins": [
        "transform-export-extensions",
        "babel-plugin-emotion",
        [ "babel-plugin-webpack-alias", { "config": "./razzle.config.js" } ],
        "inline-react-svg",
        "polished",
        "lodash",
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "media": "./src/media"
                }
            }
        ]
    ]
}